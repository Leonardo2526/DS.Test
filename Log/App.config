<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.diagnostics>
		<switches>
			<add name="ErrorSwitch" value="Error"/>
			<add name="WarinigSwitch" value="Warning"/>
		</switches>
		<sources>
			<!-- Add new listener  -->
			<source name="TraceSourceApp1"
			  switchName="WarinigSwitch"
			  switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<add name="ConsoleListener"
					  type="System.Diagnostics.ConsoleTraceListener">
						<filter type="System.Diagnostics.EventTypeFilter"
						  initializeData="Warning"/>
					</add>
					<remove name="Default"/>
				</listeners>
			</source>
			<!-- Add new listener  -->
			<source name="TraceSourceApp2"
			  switchName="ErrorSwitch"
			  switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<add name="WriteListener"
						type="System.Diagnostics.TextWriterTraceListener"
						initializeData="WriteListener.log">
					</add>
					<remove name="Default"/>
				</listeners>
			</source>
		</sources>		
		
	</system.diagnostics>
</configuration>
